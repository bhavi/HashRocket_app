<h1>Tutorials</h1>

<table>
  <tr>
   <th> Title </th>
	<th> Duration </th>
	<th> Released date </th>
	<th> Link </th>
	<th></th>
  </tr>

  <% @tutorials.each do |s| %>
  <tr>
    <td><%= s.title %></td>
    <td><%= s.duration %></td>
    <td><%= s.release_date.to_s(:long) %></td>
    <td><%= link_to s.url, s.url  %></td>
    <td> <%= link_to "Show", tutorial_path(s) %> </td>
  </tr>
<% end %>
</table>
<br />
<%= link_to "Add a Tutorial",new_tutorial_path %>
